 20      FETCH cur_PERIODE_FORFAIT_JOUEUR INTO rec_PERIODE_FORFAIT_JOUEUR;
 21      WHILE cur_PERIODE_FORFAIT_JOUEUR%FOUND LOOP
 22          IF (rec_PERIODE_FORFAIT_JOUEUR.DATEACHAT >= add_months(Today, -12)) 
 23          THEN
 24              UPDATE PERIODE_FORFAIT_JOUEUR SET DATEACHAT = add_months(DATEACHAT, 2)
 25              WHERE CURRENT OF cur_PERIODE_FORFAIT_JOUEUR;
