  2      CodeForfait OUT FORFAIT.CODE%TYPE,
  3      NewPrice OUT PERIODE.PRIX%TYPE
  4      )
  5  IS
  6      IDForfaitPlusPopulaire PERIODE.IDPERIODE%TYPE;
  7      CodeReseauPlusPopulaire ABONNEMENT_RESEAU.CODERESEAU%TYPE;
  8      CodeForfaitPlusCher FORFAIT.CODE%TYPE;
  9  BEGIN
 10  
 11      SELECT * INTO CodeReseauPlusPopulaire FROM (SELECT CODERESEAU FROM ABONNEMENT_RESEAU GROUP BY CODERESEAU ORDER BY COUNT(CODERESEAU) DESC) WHERE ROWNUM = 1;
 12  
 13      SELECT * INTO IDForfaitPlusPopulaire FROM (SELECT PERIODE.IDPERIODE FROM JOUEUR 
 14      JOIN ABONNEMENT_RESEAU ON JOUEUR.NOJOUEUR = ABONNEMENT_RESEAU.NOJOUEUR
 15      JOIN PERIODE_FORFAIT_JOUEUR ON PERIODE_FORFAIT_JOUEUR.NOJOUEUR = JOUEUR.NOJOUEUR
 16      JOIN PERIODE ON PERIODE.IDPERIODE = PERIODE_FORFAIT_JOUEUR.IDPERIODE
 17      WHERE ABONNEMENT_RESEAU.CODERESEAU = CodeReseauPlusPopulaire
 18      GROUP BY PERIODE.IDPERIODE
 19      ORDER BY COUNT(PERIODE.IDPERIODE) DESC)
 20      WHERE ROWNUM = 1;
 21  
 22      SELECT * INTO CodeForfaitPlusCher FROM (SELECT IDPERIODE FROM PERIODE GROUP BY IDPERIODE ORDER BY MAX(PRIX) DESC) WHERE ROWNUM = 1;
 23  
 24      IF (IDForfaitPlusPopulaire != CodeForfaitPlusCher) THEN
 25          UPDATE PERIODE SET PRIX = PRIX*1.1 WHERE IDPERIODE = IDForfaitPlusPopulaire; 
 26      END IF;
 27  
 28      SELECT PERIODE.PRIX, PERIODE.CODEFORFAIT INTO NewPrice, CodeForfait FROM PERIODE WHERE PERIODE.IDPERIODE = IDForfaitPlusPopulaire AND ROWNUM = 1;
 29  
 30  
 31  END;
